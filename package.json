{
  "name": "material-design-new-tab",
  "version": "2.0.0",
  "description": "A Chrome new tab page extension with Material Design",
  "homepage": "https://tab.xiejie.co",
  "keywords": [
    "chrome",
    "browser extension",
    "new tab page",
    "material design"
  ],
  "author": {
    "name": "ConanXie",
    "email": "xjj416@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ConanXie/new-tab.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --progress --color --config webpack/webpack.dev.conf.js",
    "build": "webpack --config webpack/webpack.prod.conf.js",
    "release": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./release.ps1",
    "lint": "eslint . --ext .ts,.tsx",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --colors",
    "test:coverage": "jest --silent --coverage",
    "test:clear": "jest --clearCache"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/test.shim.js",
      "<rootDir>/test/test.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^(config|utils|stores|components)(.*)": "<rootDir>/src/$1$2"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/test.preprocessor.js",
      "^.+\\.(css|less)$": "<rootDir>/test/test.ignore.js"
    },
    "testMatch": [
      "<rootDir>/(test|src)/**/*.test.(ts|tsx)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!src/utils/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/styles": "^4.11.4",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "color": "^3.1.3",
    "date-fns": "^2.22.1",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mobx-react-lite": "^3.2.0",
    "notistack": "^1.0.9",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.14.0",
    "react-loadable": "^5.5.0",
    "shortid": "^2.2.16",
    "store2": "^2.12.0",
    "typeface-roboto": "1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@hot-loader/react-dom": "^16.14.0",
    "@storybook/react": "^6.2.9",
    "@types/chrome": "0.0.143",
    "@types/classnames": "^2.3.1",
    "@types/color": "^3.0.1",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.14.8",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.13",
    "@types/react-loadable": "^5.5.5",
    "@types/shortid": "0.0.29",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.27.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.3",
    "mini-css-extract-plugin": "^1.6.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "react-hot-loader": "^4.13.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^6.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
