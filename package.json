{
  "name": "material-design-new-tab",
  "version": "2.0.0",
  "description": "A Chrome new tab page extension with Material Design",
  "homepage": "https://tab.xiejie.co",
  "keywords": [
    "chrome",
    "browser extension",
    "new tab page",
    "material design"
  ],
  "author": {
    "name": "ConanXie",
    "email": "xjj416@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ConanXie/new-tab.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --progress --color --config webpack/webpack.dev.conf.js",
    "build": "webpack --config webpack/webpack.prod.conf.js",
    "release": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./release.ps1",
    "lint": "eslint . --ext .ts,.tsx",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --colors",
    "test:coverage": "jest --silent --coverage",
    "test:clear": "jest --clearCache"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/test.shim.js",
      "<rootDir>/test/test.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^(config|utils|stores|components|styles)(.*)": "<rootDir>/src/$1$2"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/test.preprocessor.js",
      "^.+\\.(css|less)$": "<rootDir>/test/test.ignore.js"
    },
    "testMatch": [
      "<rootDir>/(test|src)/**/*.test.(ts|tsx)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!src/utils/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "color": "^4.0.1",
    "date-fns": "^2.24.0",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-react-lite": "^3.2.1",
    "notistack": "^1.0.10",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.14.0",
    "react-loadable": "^5.5.0",
    "shortid": "^2.2.16",
    "store2": "^2.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@hot-loader/react-dom": "^16.14.0",
    "@storybook/react": "^6.3.8",
    "@types/chrome": "0.0.154",
    "@types/color": "^3.0.2",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.1",
    "@types/react": "^16.14.14",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^16.9.13",
    "@types/react-loadable": "^5.5.6",
    "@types/shortid": "0.0.29",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.0",
    "mini-css-extract-plugin": "^2.3.0",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^3.2.1",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.1.0",
    "terser-webpack-plugin": "^5.2.4",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0"
  }
}
