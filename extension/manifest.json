{
  "name": "Material Design New Tab",
  "version": "2.0.0",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "logo/logo16.png",
    "128": "logo/logo128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "logo/logo48.png",
    "default_popup": "popup.html"
  },
  "options_page": "settings.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "management",
    "bookmarks",
    "geolocation",
    "notifications",
    "downloads",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost:5001; object-src 'self'"
}
