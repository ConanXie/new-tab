{
  "name": "Material Design New Tab Extension",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "An extension to replace the default new tab of Chrome",
  "icons": {
    "192": "icons/icon192.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "management",
    "bookmarks",
    "chrome://favicon/*"
  ],
  "content_security_policy": "script-src 'self' https://localhost:5001; object-src 'self'"
}