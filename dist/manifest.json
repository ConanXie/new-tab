{
  "name": "Material Design New Tab",
  "version": "1.3.0",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "256": "icons/icon@256.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "management",
    "bookmarks",
    "geolocation",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://localhost:5001; object-src 'self'"
}