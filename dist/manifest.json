{
  "name": "Material Design New Tab",
  "version": "1.2.0",
  "manifest_version": 2,
  "description": "Material Design new tab page",
  "icons": {
    "256": "icons/icon@256.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "management",
    "bookmarks",
    "chrome://favicon/*"
  ],
  "content_security_policy": "script-src 'self' https://localhost:5001; object-src 'self'"
}