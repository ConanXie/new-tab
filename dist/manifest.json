{
  "name": "Material Design New Tab",
  "version": "1.5.0",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icons/icon@16.png",
    "48": "icons/icon@48.png",
    "128": "icons/icon@128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "management",
    "bookmarks",
    "geolocation",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:5001; object-src 'self'"
}
